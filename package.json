{
  "name": "e-organico-admin-api",
  "version": "1.0.0",
  "main": "src/main.ts",
  "repository": "https://github.com/iago-f-s-e/e-organico-admin-api.git",
  "author": "iago-f-s-e <iago.fagundes@outlook.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "up:dev": "docker-compose -f docker-compose.dev.yml up",
    "up:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "down:dev": "docker-compose -f docker-compose.dev.yml down",
    "down:prod": "docker-compose -f docker-compose.prod.yml down",
    "log:dev": "docker-compose -f docker-compose.dev.yml logs",
    "log:prod": "docker-compose -f docker-compose.prod.yml logs",
    "lint": " eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typecheck": "yarn tsc --noEmit",
    "typewatch": "yarn tsc -w --noEmit",
    "prepare:husky": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "helmet": "^6.0.0",
    "ioredis": "^5.0.5",
    "pg": "^8.7.3",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.2",
    "@nestjs/testing": "^9.0.11",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.8.2"
  }
}
